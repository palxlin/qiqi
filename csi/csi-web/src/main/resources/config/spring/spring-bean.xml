<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-lazy-init="false">

    <bean id="adminConfig" class="com.csi.web.system.AdminConfig">

        <property name="systemId" value="1"></property>
        <property name="onTestMode" value="${onTestMode}"></property>
        <property name="filePath" value="${config.files.path}"></property>
        <property name="contextPath" value="${config.context.web.path"></property>
    </bean>

    <bean id="weixinConfig" class="com.csi.system.WeixinConfig">
        <property name="token" value="${weixin_token}"></property>
        <property name="appId" value="${weixin_appid}"></property>
        <property name="appSecret" value="${weixin_appSecret}"></property>
        <property name="filePath" value="${config.files.path}"></property>
    </bean>
</beans>